<div class="content">
	<div id="tableContainer">
		<table class="table table-striped table-hover table-responsive">
			<tr>
				<th>ID</th>
				<th>Full Name</th>
				<th>Email</th>
				<th class="indexButtons"></th>
			</tr>
			<% for (var i=0; i<users.length; i++){ %>
				<tr>
					<td><%= users[i].id %></td>
					<td><%= users[i].firstName %> <%= users[i].lastName %></td>
					<td><%= users[i].email %></td>
					<td class="indexButtons">
						<a class="btn btn-primary btn-xs" href="../user/show/<%= users[i].id %>">Show</a>
						<a class="btn btn-success btn-xs" href="../user/edit/<%= users[i].id %>">Edit</a>
						<a class="btn btn-danger btn-xs" href="../user/delete/<%= users[i].id %>">Delete</a>
					</td>
				</tr>
			<% } %>
		</table>
	</div>
</div>